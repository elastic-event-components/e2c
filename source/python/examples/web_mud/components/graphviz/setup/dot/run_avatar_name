digraph {
	graph [label=run_avatar_name labeljust=r]
	edge [color=blue fontcolor=blue labelheadcolor=blue]
	"•" [color=green]
	".out" [color=green]
	"•" -> ".out" [label=err color=red fontcolor=red]
	"•" -> restart [label=err color=red fontcolor=red]
	"•" [color=green]
	"•" -> set_avatar_name [label=run]
	".out" [color=green]
	set_avatar_name -> ".out" [label=ask]
	set_avatar_name -> store_avatar_name [label=store]
	".out" [color=green]
	store_avatar_name -> ".out" [label=out]
	store_avatar_name -> set_avatar_to_session [label=set_avatar_id]
	store_avatar_name -> ".next" [label=stored]
	restart -> set_avatar_name [label=next]
}
