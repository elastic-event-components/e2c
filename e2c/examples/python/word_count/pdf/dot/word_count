// word_count
digraph {
	graph [label=word_count labeljust=r]
	edge [color=blue fontcolor=blue labelheadcolor=blue]
	"•" [color=green]
	"•" -> error [label=err color=red fontcolor=red]
	"•" [color=green]
	"•" -> list_files [label=run]
	"•" [color=green]
	"•" -> end [label=end]
	".out" [color=green]
	list_files -> ".out" [label=load_file]
	list_files -> open_file [label=load_file]
	open_file -> word_count [label=out]
	".out" [color=green]
	word_count -> ".out" [label=out]
}
