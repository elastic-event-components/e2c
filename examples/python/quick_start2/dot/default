// default
digraph {
	graph [label=default labeljust=r]
	node [color=black fontcolor=black]
	edge [color=orange fontcolor=orange]
	"•" [color=orange]
	"•" -> action [label=run]
	action -> render [label=render]
	action -> log [label=log]
	".out" [color=orange]
	render -> ".out" [label=out]
	".out" [color=orange]
	log -> ".out" [label=store]
}
