// run_avatar_race
digraph {
	graph [label=run_avatar_race labeljust=r]
	edge [color=blue fontcolor=blue labelheadcolor=blue]
	"•" [color=green]
	".out" [color=green]
	"•" -> ".out" [label=err color=red fontcolor=red]
	"•" -> restart [label=err color=red fontcolor=red]
	"•" [color=green]
	"•" -> set_avatar_race [label=run]
	".out" [color=green]
	set_avatar_race -> ".out" [label=ask]
	set_avatar_race -> store_avatar_race [label=store]
	".out" [color=green]
	store_avatar_race -> ".out" [label=out]
	store_avatar_race -> get_avatar_id_from_session [label=get_avatar_id]
	store_avatar_race -> ".next" [label=stored]
	restart -> set_avatar_race [label=next]
}
