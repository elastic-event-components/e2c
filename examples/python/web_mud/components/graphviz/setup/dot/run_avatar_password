// run_avatar_password
digraph {
	graph [label=run_avatar_password labeljust=r]
	edge [color=blue fontcolor=blue labelheadcolor=blue]
	"•" [color=green]
	".out" [color=green]
	"•" -> ".out" [label=err color=red fontcolor=red]
	"•" -> restart [label=err color=red fontcolor=red]
	"•" [color=green]
	"•" -> set_avatar_password [label=run]
	".out" [color=green]
	set_avatar_password -> ".out" [label=ask]
	set_avatar_password -> store_avatar_password [label=store]
	".out" [color=green]
	store_avatar_password -> ".out" [label=out]
	store_avatar_password -> get_avatar_id_from_session [label=get_avatar_id]
	store_avatar_password -> ".next" [label=stored]
	restart -> set_avatar_password [label=next]
}
